{
  "name": "nsfw-watcher",
  "description": "File system watcher based on NSFW that emits add/change/rename/unlink events.",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "compile": "tsc --skipLibCheck && tstei",
    "compile:watch": "tsc --skipLibCheck --watch",
    "prepublishOnly": "npm run clean && npm run compile"
  },
  "bugs": {
    "url": "https://github.com/fabiospampinato/nsfw-watcher/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Fabio Spampinato",
    "email": "spampinabio@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fabiospampinato/nsfw-watcher.git"
  },
  "keywords": [
    "file-system",
    "watcher",
    "nsfw",
    "add",
    "change",
    "rename",
    "unlink"
  ],
  "dependencies": {},
  "peerDependencies": {
    "nsfw": "*"
  },
  "devDependencies": {
    "@types/node": "^12.0.2",
    "nsfw": "^1.2.2",
    "rimraf": "^2.6.3",
    "typescript": "^3.4.5",
    "typescript-transform-export-interop": "^1.0.2"
  }
}
